{
    "manifest_version": 3,
    "name": "LinkedIn Connection Navigator",
    "version": "1.0",
    "permissions": ["activeTab", "scripting", "tabs"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_title": "Open Connections"
    },
    "host_permissions": [
      "https://www.linkedin.com/*"
    ]
  }
  
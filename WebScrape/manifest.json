{
    "manifest_version": 3,
    "name": "Web Scraper",
    "version": "1.0",
    "permissions": ["activeTab", "scripting", "tabs"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_title": "My Extension"
    },
    "host_permissions": [
        "<all_urls>"
    ]
  }